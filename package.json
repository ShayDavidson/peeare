  "author": "Shay Davidson <shay.h.davidson@gmail.com>",
  "keywords": [
    "pr",
    "github",
    "cli",
    "team",
    "repo",
    "track"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ShayDavidson/peeare"
  },
  "license": "MIT",
  "main": "src/cli.js",
